{
  "env": {
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-invalid-this": "off",
    "block-scoped-var": "off",
    "prefer-const": "off",
    "no-var": "off",
    "linebreak-style": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "max-len": "off",
    "operator-linebreak": "off",
    "valid-jsdoc": "off",
    "require-jsdoc": "off"
  }
}
